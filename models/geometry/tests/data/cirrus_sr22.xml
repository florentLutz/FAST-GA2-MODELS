<FASTOAD_model>
	<data>
		<TLAR>
			<v_cruise units="kn">160.0<!--top-level requirement: cruise speed--></v_cruise>
		</TLAR>
		<geometry>
			<cabin>
				<aisle_width units="m">0.0<!--cabin aisle width--></aisle_width>
				<luggage>
					<mass_max units="kg">60.0<!--maximum luggage mass to carry--></mass_max>
				</luggage>
				<seats>
					<pilot>
						<length units="m">0.75<!--pilot seats length--></length>
						<width units="m">0.565<!--pilot seats width--></width>
					</pilot>
					<passenger>
						<NPAX_max>2.0<!--maximum number of passengers in cabin (2 pilots seats not included)--></NPAX_max>
						<count_by_row>2.0<!--passenger seats per row--></count_by_row>
						<length units="m">0.8<!--passenger seats length--></length>
						<width units="m">0.4<!--passenger seats width--></width>
					</passenger>
				</seats>
			</cabin>
			<fuselage>
				<rear_length units="m">3.275</rear_length>
			</fuselage>
			<has_T_tail>1.0<!--0=horizontal tail is attached to fuselage / 1=horizontal tail is attached to top of vertical tail--></has_T_tail>
			<horizontal_tail>
				<MAC>
					<at25percent>
						<x>
							<from_wingMAC25 units="m">3.91<!--distance along X between 25% MAC of wing and 25% MAC of horizontal tail--></from_wingMAC25>
						</x>
					</at25percent>
				</MAC>
				<area units="m**2">2.75<!--horizontal tail area--></area>
				<sweep_25 units="deg">4.0<!--sweep angle at 25% chord of horizontal tail--></sweep_25>
				<taper_ratio>0.613<!--taper ratio of horizontal tail--></taper_ratio>
				<thickness_ratio>0.12<!--thickness ratio of horizontal tail--></thickness_ratio>
			</horizontal_tail>
			<propulsion>
				<layout>3.0<!--position of engines (1=under the wing / 2=rear fuselage / 3=nose)--></layout>
				<count>1.0<!--number of engines--></count>
				<y_ratio>0.0<!--engine position with respect to total span--></y_ratio>
				<propeller_dia units="m">2.05<!--propeller diameter--></propeller_dia>
			</propulsion>
			<vertical_tail>
				<area units="m**2">1.51<!--vertical tail area--></area>
				<aspect_ratio>2.0<!--aspect ratio of vertical tail--></aspect_ratio>
				<sweep_25 units="deg">10.0<!--sweep angle at 25% chord of vertical tail--></sweep_25>
				<taper_ratio>0.502<!--taper ratio of vertical tail--></taper_ratio>
				<thickness_ratio>0.12<!--thickness ratio of vertical tail--></thickness_ratio>
			</vertical_tail>
			<wing>
				<MAC>
					<at25percent>
						<x units="m">2.65<!--X-position of the 25% of mean aerodynamic chord of wing w.r.t. aircraft nose (drives position of wing along fuselage)--></x>
					</at25percent>
				</MAC>
				<area units="m**2">13.46<!--wing reference area--></area>
				<aspect_ratio>10.12<!--wing aspect ratio--></aspect_ratio>
				<kink>
					<span_ratio>0.0<!--ratio (Y-position of kink)/(semi-span)--></span_ratio>
				</kink>
				<sweep_25 units="deg">0.0<!--sweep angle at 25% chord of wing--></sweep_25>
				<taper_ratio>0.5<!--taper ratio of wing--></taper_ratio>
				<thickness_ratio>0.12<!--mean thickness ratio of wing--></thickness_ratio>
			</wing>
		</geometry>
		<mission>
		  	<sizing>
				<main_route>
					<cruise>
					  <altitude units="ft">8000.0<!--altitude during cruise phase in sizing mission--></altitude>
					</cruise>
				</main_route>
			</sizing>
		</mission>
		<propulsion>
			<IC_engine>
				<fuel_type>1.0<!--1=gasoline ICE engine / 2=Diesel ICE engine / 3=Jet fuel A1--></fuel_type>
				<max_power units="W">231000.0<!--max power @ see level--></max_power>
				<strokes_nb>4.0<!--X-strokes engine (2.0 or 4.0)--></strokes_nb>
			</IC_engine>
		</propulsion>
	</data>
</FASTOAD_model>
